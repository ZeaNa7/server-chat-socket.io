<%- include('partials/sidebar'); -%>

<style>
    img {
        width: 100%;
        height: 50%;
        border-radius: 10px;
    }

    a {
        text-decoration: none;
        color: black;
    }

    h3 {
        margin: 10px 0 10px 0; 
    }

    .window {
        display: block;
    }

    .container {
        display: flex;
        flex-wrap: wrap;
        flex-direction: row;
        justify-content: center;
    }

    .card {
        /* Add shadows to create the "card" effect */
        box-shadow: 0 4px 8px 0 rgba(0, 0, 0, 0.2);
        transition: 0.3s;
        width: 250px;
        height: 350px;
        padding: 20px;
        border-radius: 10px;
        background-color: rgb(0, 128, 113);
        align-items: center;
        justify-content: center;
        margin: 10px 20px 0 0;
    }

    .card:hover {
        box-shadow: 0 8px 16px 0 rgba(255, 255, 255, 0.2);
    }
</style>

<body>
    <div class="window">
        <h1>Languages</h1>
        <div class="container">
            <div class="card">
                <img src="https://upload.wikimedia.org/wikipedia/commons/thumb/c/c3/Flag_of_France.svg/2560px-Flag_of_France.svg.png"
                    class="fa fa-rebel" aria-hidden="true" alt="Frensh" style="height:80%">
                <h3>Frensh</h3>
                <button type="submit"><a href="learn/frensh">Learn</a></button>
            </div>
            <div class="card">
                <img src="data:image/png;base64,"
                    alt="Thai" style="height:80%">
                <h3>Thai</h3>
                <button type="submit"><a href="learn/thai">Learn</a></button>
            </div>
            <div class="card">
                <img src="data:image/png;base64,"
                    alt="Korean" style="height:80%">
                <h3>Korean</h3>
                <button type="submit"><a href="learn/korean">Learn</a></button>
            </div>
        </div>
    </div>
</body>